{
  "compilerOptions": {
    "target": "es2020",
    "jsx": "react-jsx",
    "module": "esnext",
    "moduleResolution": "node",
    "types": ["chrome", "webpack-env", "electron"],
    "declaration": false,
    "sourceMap": false,
    "outDir": "./dist",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true
  }
}
